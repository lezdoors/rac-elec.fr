<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <defs>
    <!-- Dégradés pour les différentes parties de l'icône -->
    <linearGradient id="sky-gradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" stop-color="#2196F3" />
      <stop offset="100%" stop-color="#1565C0" />
    </linearGradient>
    
    <linearGradient id="roof-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#A0522D" />
      <stop offset="60%" stop-color="#8B4513" />
      <stop offset="100%" stop-color="#5D2906" />
    </linearGradient>
    
    <linearGradient id="wall-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#FFFFF0" />
      <stop offset="60%" stop-color="#F5F5DC" />
      <stop offset="100%" stop-color="#E8E8D0" />
    </linearGradient>
    
    <linearGradient id="door-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#A0522D" />
      <stop offset="60%" stop-color="#8B4513" />
      <stop offset="100%" stop-color="#8B4513" />
    </linearGradient>
    
    <linearGradient id="grass-gradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" stop-color="#228B22" />
      <stop offset="100%" stop-color="#006400" />
    </linearGradient>
    
    <linearGradient id="window-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#B3E5FC" />
      <stop offset="100%" stop-color="#81D4FA" />
    </linearGradient>
    
    <linearGradient id="metal-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#78909C" />
      <stop offset="50%" stop-color="#607D8B" />
      <stop offset="100%" stop-color="#546E7A" />
    </linearGradient>
    
    <!-- Effet d'ombre pour donner de la profondeur -->
    <filter id="shadow" x="-10%" y="-10%" width="120%" height="120%">
      <feDropShadow dx="0.5" dy="1" stdDeviation="0.5" flood-color="#000000" flood-opacity="0.3" />
    </filter>
  </defs>
  
  <!-- Fond circulaire -->
  <circle id="background" cx="16" cy="16" r="16" fill="#FFF" />
  
  <!-- Cercle de fond bleu -->
  <circle cx="16" cy="16" r="15" fill="url(#sky-gradient)" filter="url(#shadow)" />
  
  <!-- Gazon/sol -->
  <path d="M0,24 L32,24 L32,32 L0,32 Z" fill="url(#grass-gradient)" />
  
  <!-- Maison - structure principale -->
  <g filter="url(#shadow)" transform="translate(-2, 0)">
    <!-- Murs de la maison -->
    <rect x="8" y="14" width="12" height="10" fill="url(#wall-gradient)" stroke="#998675" stroke-width="0.3" />
    
    <!-- Toit de la maison -->
    <polygon points="6,14 14,6 22,14" fill="url(#roof-gradient)" stroke="#5D2906" stroke-width="0.3" />
    
    <!-- Porte -->
    <rect x="12" y="18" width="3" height="6" fill="url(#door-gradient)" stroke="#5D2906" stroke-width="0.3" />
    
    <!-- Poignée de porte -->
    <circle cx="13" cy="21" r="0.4" fill="#FFD700" />
    
    <!-- Fenêtres -->
    <rect x="9" y="16" width="2" height="3" fill="url(#window-gradient)" stroke="#A0522D" stroke-width="0.3" />
    <line x1="9" y1="17.5" x2="11" y2="17.5" stroke="#A0522D" stroke-width="0.3" />
    <line x1="10" y1="16" x2="10" y2="19" stroke="#A0522D" stroke-width="0.3" />
    
    <rect x="16" y="16" width="2" height="3" fill="url(#window-gradient)" stroke="#A0522D" stroke-width="0.3" />
    <line x1="16" y1="17.5" x2="18" y2="17.5" stroke="#A0522D" stroke-width="0.3" />
    <line x1="17" y1="16" x2="17" y2="19" stroke="#A0522D" stroke-width="0.3" />
    
    <!-- Fenêtre grenier (triangulaire) -->
    <polygon points="13,10 14,8 15,10" fill="url(#window-gradient)" stroke="#A0522D" stroke-width="0.3" />
  </g>
  
  <!-- Pylône électrique -->
  <g filter="url(#shadow)" transform="translate(4, 0)">
    <!-- Structure principale du pylône -->
    <rect x="20" y="10" width="1.5" height="14" fill="url(#metal-gradient)" stroke="#455A64" stroke-width="0.2" />
    
    <!-- Bras horizontaux du pylône -->
    <rect x="17" y="12" width="8" height="0.8" fill="url(#metal-gradient)" stroke="#455A64" stroke-width="0.2" />
    <rect x="18" y="15" width="6" height="0.8" fill="url(#metal-gradient)" stroke="#455A64" stroke-width="0.2" />
    <rect x="19" y="18" width="4" height="0.8" fill="url(#metal-gradient)" stroke="#455A64" stroke-width="0.2" />
    
    <!-- Fils électriques -->
    <path d="M17,12.4 C15,13 13,14 11,16" fill="none" stroke="#555" stroke-width="0.3" />
    <path d="M25,12.4 C27,13 29,14 31,16" fill="none" stroke="#555" stroke-width="0.3" />
    
    <path d="M18,15.4 C16,16 15,17 14,19" fill="none" stroke="#555" stroke-width="0.3" />
    <path d="M24,15.4 C26,16 27,17 28,19" fill="none" stroke="#555" stroke-width="0.3" />
    
    <path d="M19,18.4 C18,19 17,20 16,22" fill="none" stroke="#555" stroke-width="0.3" />
    <path d="M23,18.4 C24,19 25,20 26,22" fill="none" stroke="#555" stroke-width="0.3" />
  </g>
  
  <!-- Soleil -->
  <circle cx="5.5" cy="7.5" r="1.8" fill="#FFD700" />
  <g fill="none" stroke="#FFD700" stroke-width="0.4">
    <line x1="5.5" y1="4.5" x2="5.5" y2="3.5" />
    <line x1="5.5" y1="11.5" x2="5.5" y2="10.5" />
    <line x1="2.5" y1="7.5" x2="3.5" y2="7.5" />
    <line x1="8.5" y1="7.5" x2="7.5" y2="7.5" />
    <line x1="3.5" y1="5.5" x2="4.2" y2="6.2" />
    <line x1="7.5" y1="9.5" x2="6.8" y2="8.8" />
    <line x1="3.5" y1="9.5" x2="4.2" y2="8.8" />
    <line x1="7.5" y1="5.5" x2="6.8" y2="6.2" />
  </g>
  
  <!-- Nuage léger -->
  <g fill="#FFF" opacity="0.7" filter="url(#shadow)">
    <circle cx="25" cy="5" r="1.2" />
    <circle cx="27" cy="5" r="1.5" />
    <circle cx="23.5" cy="5.5" r="1" />
    <circle cx="26" cy="4.5" r="1.3" />
  </g>
  
  <!-- Media Query pour le mode sombre/clair -->
  <style>
    @media (prefers-color-scheme: light) {
      #background { fill: #FFF; }
    }
    @media (prefers-color-scheme: dark) {
      #background { fill: #0A1929; }
      stop[offset="0%"][stop-color="#2196F3"] { stop-color: #4FC3F7; }
      stop[offset="100%"][stop-color="#1565C0"] { stop-color: #1E88E5; }
      stop[offset="0%"][stop-color="#228B22"] { stop-color: #43A047; }
      stop[offset="100%"][stop-color="#006400"] { stop-color: #2E7D32; }
    }
  </style>
</svg>